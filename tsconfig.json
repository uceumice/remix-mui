{
  "compilerOptions": {
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ES2019"
    ],
    "module": "CommonJS",
    "esModuleInterop": true,
    "moduleResolution": "Node",
    "target": "ES2019",
    "strict": true,
    "skipLibCheck": true,
    "declaration": true,
    "jsx": "react-jsx",
    "types": [
      "@remix-run/dev"
    ],
    "outDir": "out"
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.tsx"
  ],
  "exclude": [
    "node_modules"
  ]
}